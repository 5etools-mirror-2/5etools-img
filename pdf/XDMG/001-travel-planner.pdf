    <script>
    initializeBanner('d6eeb08f-2e21-41ea-9076-2f6bb0b79497', '', '');

    function initializeBanner(uniqueIdentifier, bannerId, bannerName) {
        var banner = document.querySelectorAll(`[data-banner-identifier='${uniqueIdentifier}']`)[0];
        var bannerCookieName = `ddbSiteBanner:${uniqueIdentifier}`;

        banner.querySelector('.ddb-site-banner--dismiss').addEventListener('click', function() {
            dismissBanner(banner); 
        });
        
        var links = banner.querySelectorAll('.ddb-site-banner a');
        links.forEach(l => {
            l.addEventListener('click', function() {
                pushPromotionClickToDataLayer(bannerId, bannerName);
            });
        });
        
        if (bannerIsDismissed()) {
            hideBanner(banner);
        } else {
            // push promotion details to data layer
            pushPromotionDetailToDataLayer(bannerId, bannerName);
        }

        function dismissBanner(banner) {
            setBannerDismissalCookie();
            hideBanner(banner);
            return true;
        }

        function bannerIsDismissed() {
            return document.cookie.indexOf(bannerCookieName) > -1;
        }

        function hideBanner(banner) {
            banner.style.display = 'none';
            repositionSite();
            return true;
        }

        function setBannerDismissalCookie() {
            document.cookie = bannerCookieName + '=true;expires=' + timeTravel(new Date(), 7) + ';path=/';
            return true;
        }

        function timeTravel(startDate, daysToTravel) {
            return new Date(startDate.setDate(startDate.getDate() + daysToTravel)).toUTCString();
        }

        function repositionSite(position, top) {
            var body = document.querySelector('body');

            body.style.top = (top || 0) + 'px';
            body.style.position = position || 'initial';
        }
    }
    </script>

